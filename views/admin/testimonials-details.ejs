<div class="p-3">
    <% if (token.role===3) { %>
<a href="/admin/testimonials" class="nav-link text-decoration-none text-dark fs-4 ">
    <img src="/static/img/left-arrow.png" class="me-2" height="20px" width="20px">Testimonials</a>
    <%} else { %>
        <a href="/admin/schools" class="nav-link text-decoration-none text-dark fs-4 ">
            <img src="/static/img/left-arrow.png" class="me-2" height="20px" width="20px">Testimonials</a>
            <% } %>     
<div class="mt-5">
    <div class="text-center">
        <img src="/<%= testimonialsList.profile; %>" class="rounded"  width="300px" height="200">
      </div>
<div class="mt-5">
    <span class="text-dark"><b>Testimonial Name</b></span> :
    <span class="ms-3"><%= testimonialsList.name; %></span>
</div>
<div class="mt-2">
    <span class="text-dark"><b>Designation</b></span> :
    <span class="ms-3"><%= testimonialsList.designation; %></span>
</div>
<div class="mt-2">
    <span class="text-dark"><b>Description</b></span> :
    <span class="ms-3"><%- unescape(testimonialsList.description) %></span>
</div>
</div>
</div>